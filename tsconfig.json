{
    "include": [
        "./src/**/*.ts"
    ],
    "compilerOptions": {
        "target": "es6",
        "esModuleInterop": true,
        "module": "CommonJS",
        "outDir": "dist",
        "sourceMap": true,
        "allowJs": false,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "noUncheckedIndexedAccess": true
    },
}